{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "no-undef": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
    "ignorePackages": false
  }
}
